function button_click(var_x,var_y)
{

	if( var_x != 1 )
		var change_flag = is_change();

	document.form_main.P_submit_value.value = "";
	switch(var_x){
		case 9999:
			document.form_main.action = "../error.php";
			break;
		case 'back': //戻る

			if( change_flag == true )
			{
				if( confirm('更新して移動しますか？') == true )
				{
					document.form_main.P_back_flag.value = 1;
					button_click(1,0);
					return false;
				}
				else
				{
					if( confirm('更新せずに移動しますか？') == false )
						return false;
				}
			}

			document.form_main.action = "./0011_gamemenu.php";
			break;
		case 1: //種目設定：登録処理
			if( postdata_check() == false )
				return false;
			document.form_main.P_submit_value.value = "REGIST GAME_EVENT";
			document.form_main.action = "./0013_event.php";
			break;
		case 2: //種目設定：種目自動作成処理
			if( postdata_check() == false )
				return false;

			document.form_main.P_submit_sub.value = var_y;
			document.form_main.P_submit_value.value = "CREATE NEW GAME_EVENT";
			document.form_main.action = "./0013_event.php";
			break;
		case 3: //種目設定：検索処理

			if( change_flag == true )
			{
				if( confirm('更新して移動しますか？') == true )
				{
					document.form_main.P_back_flag.value = 2;
					button_click(1,0);
					return false;
				}
				else
				{
					if( confirm('更新せずに移動しますか？') == false )
						return false;
				}
			}

			document.form_main.action = "./0013_event.php";
			break;
		default:
			document.form_main.action = "../index.php";
			break;
	}
	document.form_main.submit();
	return false;
}

function postdata_check()
{
	return true;
}
