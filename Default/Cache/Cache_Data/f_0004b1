function del_rows(obj,row)
{
	var delid = document.getElementById('id_pid_' + row).value;
	if( delid != "" )
	{
		if( confirm('削除しますか？（削除しても登録を実行するまで確定されません。）') == false )
			return false;

		document.getElementById('id_delchk_' + row).value = 'ON';
		document.getElementById('id_row_' + row).style.display = 'none';

		var target = document.getElementById('id_addb_' + row);
		target.parentNode.removeChild(target);
	}
	else
	{
		var target = document.getElementById('id_row_' + row);
		while(target.firstChild) target.removeChild(target.firstChild);
		target.parentNode.removeChild(target);

		document.form_main.P_game_event_rows.value = Number(document.form_main.P_game_event_rows.value) - 1;
	}
	var btn = document.getElementsByName('name_addb');
	btn[btn.length-1].style.display = '';
	set_event_select();
}
var event_chenge_flag = 0;
var ev_sel_flag = 0;
function set_event_select()
{
	if( ev_sel_flag == 0 )
	{
		rows = 136;
		ev_sel_flag = 1;
		var ev_obj = document.getElementsByName('P_e_code[]');
		var disp_evlist = new Array();
		//全行の種目コード一覧取得
		for(i=0;i<ev_obj.length;i++)
		{
			disp_evlist.push(ev_obj[i].value)
		}
		//各行の処理
		for(i=0;i<ev_obj.length;i++)
		{
			if( ev_obj[i].type == 'select-one')
			{
				sel_id = ev_obj[i].value;
				cnt = ev_obj[i].length;
				for(j=cnt-1;j>=0;j--){
					ev_obj[i].options[j] = null;
				}
				ev_obj[i].options[0] = new Option('','');
				opt = 1;
				for(j=0;j<rows;j++)
				{
					tmp_flag = 0;
					for(k=0;k<disp_evlist.length;k++)
					{
						if(event_codes[j].substr( 0, 4 ) == disp_evlist[k].substr( 0, 4 ))
						{
							if( sel_id != event_codes[j] )
							{
								tmp_flag = 1;
								break;
							}
						}
					}
					if( tmp_flag == 0 )
					{
						ev_obj[i].options[opt] = new Option(event_names[j],event_codes[j]);
						if( sel_id == event_codes[j] ){
							ev_obj[i].options[opt].selected = true;
						}
						opt++;
					}
				}
			}
		}
		ev_sel_flag = 0;

		event_chenge_flag = 1;
	}
}

var form_element = new Array('P_entry_category[]','P_entry_grade_min[]','P_entry_grade_max[]','P_entry_age_min[]','P_entry_age_max[]');
function is_change()
{

	// 指定フォーム要素対象版
	var cnt = form_element.length;
	for(var i=0;i<cnt;i++)
	{
		if( document.form_main.elements[form_element[i]] )
		{
			var frm = document.form_main.elements[form_element[i]];

			var tmptag = document.getElementsByName( form_element[i] ) ;


			if( frm.length > 1 )
			{
				for(var j=0;j<frm.length;j++)
				{
					if( frm[j].tagName == "INPUT" )
					{
						if( frm[j].getAttribute("type") == "text" && frm[j].value != frm[j].defaultValue )
						{
							return true; // 変更あり
						}
						else if( frm[j].getAttribute("type") == "checkbox" && frm[j].checked != frm[j].defaultChecked )
							return true; // 変更あり
						else if( frm[j].getAttribute("type") == "radio" && frm[j].checked != frm[j].defaultChecked )
							return true; // 変更あり
					}
					else if( frm[j].tagName == "SELECT" && frm[j].options[frm[j].selectedIndex].defaultSelected == false )
					{
						return true; // 変更あり
					}
					else if( frm[j].tagName == "TEXTAREA" && frm[j].value != frm[j].defaultValue )
						return true; // 変更あり
				}
			}
			else
			{

				if( frm.tagName == "INPUT" )
				{
					if( frm.getAttribute("type") == "text" && frm.value != frm.defaultValue )
					{
						return true; // 変更あり
					}
					else if( frm.getAttribute("type") == "checkbox" && frm.checked != frm.defaultChecked )
						return true; // 変更あり
					else if( frm.getAttribute("type") == "radio" && frm.checked != frm.defaultChecked )
						return true; // 変更あり
				}
				else if( frm.tagName == "SELECT" && frm.options[frm.selectedIndex].defaultSelected == false )
				{
					return true; // 変更あり
				}
				else if( frm.tagName == "TEXTAREA" && frm.value != frm.defaultValue )
					return true; // 変更あり
			}
		}
	}

	if(event_chenge_flag == 1)
		return true; // 変更あり
		

	return false;
}

function onload_event()
{
	return;
}

