function submit_link(id,mode) {
  document.form1.target = "";
  if( mode == 0 ){
	document.form1.action = "gamedata.php?gid=" + id;
  }else if( mode == 1 ){
	document.form1.target = "AR_data";
	document.form1.action = "resultdata.php";
  }else if( mode == 3 ){
	document.form1.action = "gamelist.php";
