	document.form1.rec_use.value = String(mode).substring(2,String(mode).length-1);
	document.form1.target = "AR_data";
	document.form1.action = "recorddata.php";
  }else if( mode == 6 ){
	document.form1.target = "_blank";
	document.form1.action = "timetableprint.php";
  }else{
