		{
			resulttotal.className="printButton backDGreen width120 paddingL25";
			resulttotal.onclick=function(){print_job_create_click(61);}
		}
		else
		{
			resulttotal.className="printButton backGrayC width120 paddingL25";
			resulttotal.onclick=function(){return false;}
		}

		if(e_code.substr(0,1) == 'T' )
		{
			ekidentotal.className="printButton backDGreen width120 paddingL25";
			ekidentotal.onclick=function(){print_job_create_click(192);}

		}


		if(round_id > 1)
		{
			resultplus.className="printButton backDGreen width120 paddingL25";
			resultplus.onclick=function(){print_job_create_click(70);}
		}
	}
	else
	{

		resultlist.className="printButton backGrayC width120 paddingL25";
		resulttotal.className="printButton backGrayC width120 paddingL25";
		resultgroup.className="printButton backGrayC width120 paddingL25";
		award.className="printButton backGrayC width120 paddingL25";

		resultlist.onclick=function(){return false;}
		resulttotal.onclick=function(){return false;}
		resultgroup.onclick=function(){return false;}
		award.onclick=function(){return false;}


	}


	if(e_code.substr(0,3) == 'FJH')
	{
		startfield.className="printButton backDGreen width120 paddingL25";
		startfield.onclick=function(){print_job_create_click(40);}

		if(status >= 6  || game_ope_mode == 1)
		{
			resultfield.className="printButton backDGreen width120 paddingL25";
			resultfield.onclick=function(){print_job_create_click(41);}
		}
	}
	else if(e_code.substr(0,2) == 'FJ')
	{
		startfield.className="printButton backDGreen width120 paddingL25";
		startfield.onclick=function(){print_job_create_click(42);}
		if(status >= 6 || game_ope_mode == 1)
		{
			resultfield.className="printButton backDGreen width120 paddingL25";
			resultfield.onclick=function(){print_job_create_click(43);}
		}
	}
	else if(e_code.substr(0,2) == 'FT')
	{
		startfield.className="printButton backDGreen width120 paddingL25";
		startfield.onclick=function(){print_job_create_click(44);}

		if(status >= 6  || game_ope_mode == 1)
		{
			resultfield.className="printButton backDGreen width120 paddingL25";
			resultfield.onclick=function(){print_job_create_click(45);}
		}
	}


	if(((status >= 6 && min_status >= 6  ) ||  game_ope_mode == 1) && (e_code.substr(0,1) != 'F' && e_code.substr(0,1) != 'D' && e_code.substr(0,1) != 'T' && e_code.substr(0,1) != 'T'  ))
	{

		resultgroup.className="printButton backDGreen width120 paddingL25";
		resultgroup.onclick=function(){print_job_create_click(62);}
	}


	if(e_comb > 0 || e_code.substr(0,1) == 'M' )
	{
			combtotal.className="printButton backDGreen width120 paddingL25";
			combtotal.onclick=function(){print_job_create_click(80);}

			if(e_code.substr(0,1) != 'M')
			{
				combresult.className="printButton backDGreen width120 paddingL25";
				combresult.onclick=function(){print_job_create_click(81);}
			}
	}

}



function onload_event()
{
	slit_timer();race_timer();
}


function button_copy()
{


	//着順、レーン、記録をコピーします
	var cnt = 20;

	buf = "";

	var rank = document.getElementsByName( "P_rank[]" ) ;
	var lane = document.getElementsByName( "P_lane[]" ) ;
	var rec = document.getElementsByName( "P_rec_str[]" ) ;


	//レーンとナンバーと記録を取得する
	for(i=0;i<cnt;i++)
	{
		buf = buf + rank[i].value + "\t";

		buf = buf + lane[i].value + "\t";

		buf = buf + rec[i].value + "\r\n";
	}

	var copyarea = document.getElementById("copyarea");

    copyarea.value = buf;
    copyarea.select();
    document.execCommand("copy");

	return;
}


function button_paste(mode)
{

	var p_rank = document.getElementsByName( "P_rank[]" ) ;
	var p_lane = document.getElementsByName( "P_lane[]" ) ;
	var p_rec = document.getElementsByName( "P_rec_str[]" ) ;
	var p_numbercard = document.getElementsByName( "P_numbercard[]" ) ;

	if(mode == 1)
	{
		// クリップボードからデータ取得
		var copyarea = document.getElementById("copyarea");
	    copyarea.select();
	    document.execCommand("paste");
	}
	else
	{
		// クリップボードからデータ取得
		var copyarea = document.getElementById("copyarea2");
		document.getElementById('id_dialogbox_paste').style.display = 'none';
	}

    buf = copyarea.value;


	if(buf == null)
		return;

	if(buf.length < 20)
		return;

	linedata=buf.split("\n");

	cnt=linedata.length - 1;
	var rec = new Array(cnt);
	var num = new Array(cnt);
	var pos = new Array(cnt);
	var rec_only = -1;

	for(i=0;i<cnt;i++)
	{
		sdata=linedata[i].split("\t");
		if(sdata.length<3)
		{
			if( rec_only == 0 )
			{
				cnt=i;
				break;
			}
			else
			{
				rec_only = 1;
				rec[i] = sdata[0];
			}
		}
		else
		{
			if( rec_only == 1 )
			{
				cnt=i;
				break;
			}
			else
			{
				rec_only = 0;

				pos[i] = sdata[0];
				num[i] = sdata[1];
				rec[i] = sdata[2];

				//20140906 記録の左右のスペースをとってスペースで分割して２の場合、２つ目で番号を置き換える
				rec[i] = rec[i].replace(/(^\s+)|(\s+$)/g, "");

				rdata=rec[i].split(" ");
				if(rdata.length>=2)
				{
					num[i] = rdata[rdata.length-1];
					rec[i] = rdata[0];
				}
			}
		}
	}

	var cnt2 = 20;

	if( rec_only == 0 )
	{
		//レーンとナンバーのどちらに一致するか調べる
		var lan = new Array(cnt2);
		var bib = new Array(cnt2);

		//レーンとナンバーと記録を取得する
		for(i=0;i<cnt2;i++)
		{
			lan[i]=p_lane[i].value;

			bib[i]=p_numbercard[i].value;
		}

		cnt_lan=0;
		cnt_bib=0;
		for(i=0;i<cnt;i++)
		{
			for(j=0;j<cnt2;j++)
			{
				if(parseInt(num[i],10) == parseInt(lan[j],10))
				{
					cnt_lan++;
					break;
				}
			}
			for(j=0;j<cnt2;j++)
			{
				if(parseInt(num[i],10) == parseInt(bib[j],10))
				{
					cnt_bib++;
					break;
				}
			}
		}
		for(i=0;i<cnt2;i++)
		{
			for(j=0;j<cnt;j++)
			{
				if(cnt_lan>cnt_bib)
				{
					if(parseInt(lan[i],10)==parseInt(num[j],10))
					{

						p_rank[i].value = pos[j];

						p_rec[i].value = rec[j];

						break;
					}
				}
				else
				{
					if(parseInt(bib[i],10)==parseInt(num[j],10))
					{
						p_rank[i].value = pos[j];

						p_rec[i].value = rec[j];

						break;
					}
				}
			}
		}
	}
	else if( rec_only == 1 )
	{
		//記録を画面記載順に貼り付け
		if( cnt == cnt2*2 )
		{
			var ii = 0;
			//フリガナあり
			for(i=1;i<cnt;i+=2)
			{
				p_rec[ii].value = rec[i];
				ii++;
			}
		}
		else if( cnt == cnt2 )
		{
			//フリガナなし
			for(i=0;i<cnt;i++)
			{
				p_rec[i].value = rec[i];
			}
		}
	}
	return;
}

function official_change(obj , row)
{
	var select_off = obj.value;
	var rank = document.getElementsByName( "P_rank[]" ) ;
	var rec = document.getElementsByName( "P_rec_str[]" ) ;
	var wind = document.getElementsByName( "P_wind[]" ) ;

	if(select_off == 9)
	{
		rank[row].value = "-";
		rank[row].readOnly = "true";
		rec[row].value = "";
		rec[row].readOnly = "true";
		if(wind[row])
		{
			wind[row].value = "";
			wind[row].readOnly = "true";
		}
		document.getElementById('id_row_' + row).style.backgroundColor = "#999999";
	}
	else
	{
		rank[row].readOnly = "";
		rec[row].readOnly = "";
		if(wind[row])
		{
			wind[row].readOnly = "";
		}
		document.getElementById('id_row_' + row).style.backgroundColor = "#FFFFFF";
	}
	return;
}


function mouseclick_status(status,mode,obj){




	if( mode == 1 )
	{

		if(status < 2)
			obj.style.backgroundColor = "#FFDEAD";
		else if(status < 6)
			obj.style.backgroundColor = "#f2fffc";
		else if(status < 9)
			obj.style.backgroundColor = "#ccffcc";
		else
			obj.style.backgroundColor = "#d6cbc7";

		obj.style.color = "#000000";

	}
	else
	{
		if(status < 2)
			obj.style.backgroundColor = "#FF9900";
		else if(status < 6)
			obj.style.backgroundColor = "#4472c4";
		else if(status < 9)
			obj.style.backgroundColor = "#00CC00";
		else
			obj.style.backgroundColor = "#333333";

		obj.style.color = "#FFFFFF";


	}
}

var clicked = false;    // クリック状態を保持するフラグ


function button_status(status,cnt,mode) {
    // クリックフラグが立っている状態でのクリック
    //     -> ダブルクリック



    if (clicked) 
	{

	
		switch(status)
		{
			case 3.2: // 到着済
			case 3.4: // 紹介済
			case 3.6: // 競技中
			case 3.8: // 競技終了
			case 4: // 入力
			case 5.5: // 訂正
			case 6: // 確認
			case 7: // 印刷
			case 8: // 通告済
			case 8.5: // 表彰済

				var jikan= new Date();

				//時・分・秒を取得する
				var hour = jikan.getHours();
				var minute = jikan.getMinutes();
				var second = jikan.getSeconds();

				var time = hour +":"+ minute; 

				var this_race_status = 2;


				document.getElementById('id_dialogbox_status_time').style.display = '';
				document.getElementById('id_select_status_str').innerHTML = document.getElementById('id_status_str_' + cnt).innerHTML;
				if(document.getElementById('id_status_time_' + cnt).value == "" && this_race_status <= status)
					document.form_main.P_race_status_time.value = time;
				else
					document.form_main.P_race_status_time.value = document.getElementById('id_status_time_' + cnt).value;

				//document.form_main.P_race_select_status.value = document.getElementById('id_status_time_' + cnt).value;
				document.getElementById('id_status_time_button').onclick=function(){button_status(status,cnt,1);}

				break;
			default:
				break;
		}

        clicked = false;
        return;
    }

    // シングルクリックを受理、300ms間だけダブルクリック判定を残す
    clicked = true;
    setTimeout(function () {
        // ダブルクリックによりclickedフラグがリセットされていない
        //     -> シングルクリックだった
        if (clicked) 
		{

			var change_flag = is_change();
			document.form_main.target = "";
			document.form_main.P_submit_sub.value = "";
			document.form_main.P_submit_value.value = "";

			if( change_flag == true )
			{
				if( confirm('画面の内容を変更しています。更新しますか？\nキャンセルすると変更内容を更新せずに処理します。') == true )
				{
					button_click(2,1);
					return false;
				}
			}
			document.form_main.P_submit_value.value = "REGIST RESULT_TABLE STATUS";
			document.form_main.P_submit_onload.value = "";
			document.form_main.P_set_status.value = status;

			if(mode == 0)
			{
				document.form_main.P_race_status_time.value = document.getElementById('id_status_time_' + cnt).value;
				document.form_main.P_status_change_mode.value = 0;
				 
			}
			else
			{
				document.form_main.P_status_change_mode.value = 1;

			}

			document.form_main.action = "./0020_recinput.php";


			document.form_main.submit();
//			return false;

        }

        clicked = false;
    }, 300);
}

var status_flag=0;

function mouseover_status(){
  status_flag = 1;
}
function mouseout_status(){
  status_flag = 0;
}


function close_status()
{

	if(status_flag == 1)
	{
		return;
	}
	else
	{
		document.getElementById("id_dialogbox_status").style.display="none";

	}

}

function point_rank(obj)
{
	var cnt = 20;

	if(obj.checked == false)
	{
		for(i=0;i<cnt;i++)
		{
			document.getElementById('id_point_rank_' + i).readOnly = '';
		}
	}
	else
	{
		for(i=0;i<cnt;i++)
		{
			document.getElementById('id_point_rank_' + i).readOnly = 'true';
		}
	}

}


function checkForm($this)
{
	var str=$this.value;
	while(str.match(/[^0-9\d\.()]/))
	{
		str=str.replace(/[^0-9\d\.()]/,"");
	}
	$this.value=str;
}

