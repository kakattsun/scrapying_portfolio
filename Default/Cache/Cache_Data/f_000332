function button_click(var_x,var_y)
{
	if( var_x != 2 )
		var change_flag = is_change();
	else if( var_x == 1 && var_y == 1 )
		var change_flag = false;

	document.form_main.target = "";
	document.form_main.P_submit_sub.value = "";
	document.form_main.P_submit_value.value = "";
	switch(var_x){
		case 9999:
			document.form_main.action = "../error.php";
			break;
		case 'back': //戻る
		case 99: //戻る
			if( change_flag == true )
			{
				if( confirm('更新して移動しますか？') == true )
				{
					document.form_main.P_submit_onload.value = "button_click(99,0);";
					button_click(2,1);
					return false;
				}
				else
				{
					if( confirm('更新せずに移動しますか？') == false )
						return false;
				}
			}
			document.form_main.P_submit_onload.value = "";
