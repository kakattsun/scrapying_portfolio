	combtotal.onclick=function(){return false;}
	combresult.onclick=function(){return false;}
	startfield.onclick=function(){return false;}
	resultfield.onclick=function(){return false;}
	ekidentotal.onclick=function(){return false;}
	award.onclick=function(){return false;}

	if(status >= 6  || game_ope_mode == 1)
	{

		resultlist.className="printButton backDGreen width120 paddingL25";
		resultgroup.className="printButton backGrayC width120 paddingL25";

		resultlist.onclick=function(){print_job_create_click(60);}
		resultgroup.onclick=function(){return false;}

		award.className="printButton backDGreen width120 paddingL25";
		award.onclick=function(){print_job_create_click(63);}


