function button_click(var_x,var_y)
{

	document.form_main.P_submit_value.value = "";
	document.form_main.P_select_game_race_id.value = "";
	document.form_main.target = "";

	if( var_x != 1 && var_x != 5 && var_x != 7 )
	{
		var change_flag = is_change();

		if( change_flag == true )
		{
			if( confirm('更新して移動しますか？') == true )
			{
				switch(var_x){

					case 'back': //戻る
						document.form_main.P_back_flag.value = 1;
						break;
					case 2: //レース設定＿タイムテーブルへ移動
						document.form_main.P_submit_onload.value = "button_click(2,0);";
						break;
					case 3: //レース設定＿ラウンド：検索処理
						document.form_main.P_submit_onload.value = "button_click(3,0);";
						break;
					case 4: //レース設定＿ラウンド：自動展開
						document.form_main.P_submit_onload.value = "button_click(4,0);";
						break;
					case 6: //班組へ移動
						document.form_main.P_select_game_race_id.value = var_y;
						document.form_main.P_submit_onload.value = "button_click(6,"+ var_y +");";
						break;
					case 8: //種別クラス画面へ移動
						document.form_main.P_submit_onload.value = "button_click(8,0);";
						break;
					default:
						document.form_main.action = "../index.php";
						break;
				}

				button_click(1,0);
				return false;
			}
			else
			{
				if( confirm('更新せずに移動しますか？') == false )
					return false;
			}
		}
	}

	switch(var_x){
		case 9999:
			document.form_main.action = "../error.php";
			break;
		case 'back': //戻る
			document.form_main.action = "./0011_gamemenu.php";
			break;
		case 1: //レース設定＿ラウンド：登録処理
			if( postdata_check() == false )
				return false;
			document.form_main.P_submit_value.value = "REGIST GAME_RACE ROUND";
			document.form_main.action = "./0014_race_round.php";
			break;
		case 2: //レース設定＿タイムテーブルへ移動
			document.form_main.action = "./0014_race_schedule.php";
			break;
		case 3: //レース設定＿ラウンド：検索処理
			document.form_main.action = "./0014_race_round.php";
			break;
		case 4: //レース設定＿ラウンド：自動展開
			if( postdata_check() == false )
				return false;

			if( confirm('種目とエントリー人数から自動でラウンド設定します。現在の情報は一旦クリアされますがよろしいですか？') == false )
				return false;

			document.form_main.P_submit_value.value = "AUTO SET ROUND";
			document.form_main.action = "./0014_race_round.php";
			break;
		case 5: //タイムテーブル出力(v2)
			document.form_main.target = "ARV2_WINDOW";
			document.form_main.action = "../comp/timelist.php";
			break;
		case 6: //班組へ移動
			document.form_main.P_select_game_race_id.value = var_y;
			document.form_main.action = "./0017_programinput.php";
			break;

		case 7: //v2大会記録
			document.form_main.target = "_blank";
			document.form_main.action = "0009_baserecinput.php";
			document.form_main.P_submit_sub.value = "REC GAME";
			break;
		case 8: //種別クラス画面へ移動

			document.form_main.action = "./0022_rec.php";
			break;
		default:
			document.form_main.action = "../index.php";
			break;
	}
	document.form_main.submit();
	return false;
}

function postdata_check()
{
	return true;
}

function print_button_excel(var_x,var_y)
{
	document.form_main.P_submit_value.value = "";

	document.form_main.target = "ARV2_WINDOW";
	document.form_main.action = "../comp/timelist.php";

	switch(var_x){

		case 1: //v2タイムテーブル EXCEL
			document.form_main.submit_kbn.value = "EXCEL";
			break;
		case 2: //v2タイムテーブル HTML
			document.form_main.submit_kbn.value = "HTML";
			break;
		default:
			document.form_main.action = "../index.php";
			break;
	}
	document.form_main.submit();
	return false;
}

var addrow = 11;
function add_rows(obj,row)
{
	
	var isIE =/*@cc_on!@*/false;
	var tbodyElement,trElement,tdElement,inputElement;
	var thisrow = addrow;

	//ラウンドの情報を格納しておく
	var r_cnt = 8;
	var r_name = new Array('決勝','準決勝','準々決勝','2次予選','予選','予備予選','その他','タイムレース決勝','');
	var r_code = new Array('1','2','3','4','5','6','7','8','');

	var sel_r_code = new Array();
	var sel_r_cnt = 0;

	//画面の情報を格納する
	var sex = document.getElementsByName( "P_sex[]" ) ;
	var class_id = document.getElementsByName( "P_e_class[]" ) ;
	var e_comb = document.getElementsByName( "P_e_comb[]" ) ;
	var e_code = document.getElementsByName( "P_e_code[]" ) ;
	var round_id = document.getElementsByName( "P_round_id[]" ) ;
	var max_race = document.getElementsByName( "P_max_race[]" ) ;
	var advance_rank = document.getElementsByName( "P_advance_rank[]" ) ;
	var advance_num = document.getElementsByName( "P_advance_num[]" ) ;
	var advance_rec = document.getElementsByName( "P_advance_rec[]" ) ;

	tbodyElement = document.getElementById("id_game_race_tbody");

	//よくわからないけど画面の値を入れなおしている
	var ins_index = -1;
	for(i=0;i<tbodyElement.rows.length;i++)
	{
		if( tbodyElement.rows[i].id == "id_row_" + row )
		{
			ins_index= i+1;

			for(j=0; j < tbodyElement.rows.length ;j++)
			{
				if(sex[i].value == sex[j].value && class_id[i].value == class_id[j].value && e_comb[i].value == e_comb[j].value&& e_code[i].value == e_code[j].value )
				{
					sel_r_code[sel_r_cnt]= round_id[j].value;
					sel_r_cnt++;
				}
			}
		}
	}

	trElement = tbodyElement.insertRow(ins_index);

	//コントロールの定義をしている？
	// 1
	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='hidden' name='P_id[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "hidden";
		inputElement.name = "P_id[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.id = "id_pid_" + thisrow;
	inputElement.value = "";

	if(isIE){
		inputElement = document.createElement("<input type='hidden' name='P_delete_check[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "hidden";
		inputElement.name = "P_delete_check[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.value = "";

	if(isIE){
		inputElement = document.createElement("<input type='hidden' name='P_first_race[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "hidden";
		inputElement.name = "P_first_race[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.value = "";

	//追加するコントロールの定義をしている？
	if(isIE){
		inputElement = document.createElement("<input type='button'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "button";
	}
	tdElement.appendChild(inputElement);
	inputElement.id = "id_delb_" + thisrow;
	inputElement.value = "削除";
	inputElement.onclick = function(){del_rows(inputElement,thisrow);}
	inputElement.className = "inputButton width50";
	trElement.appendChild(tdElement);
	tdElement.className = "width150 paddingL15";

	// 2
	tdElement = document.getElementById("id_psex_" + row).cloneNode(true);
	trElement.appendChild(tdElement);
	tdElement.id = "id_psex_" + thisrow;
	tdElement.className = "width100";

	// 3
	tdElement = document.getElementById("id_peclass_" + row).cloneNode(true);
	trElement.appendChild(tdElement);
	tdElement.id = "id_peclass_" + thisrow;
	tdElement.className = "width120";

	// 4
	tdElement = document.getElementById("id_pecode_" + row).cloneNode(true);
	trElement.appendChild(tdElement);
	tdElement.id = "id_pecode_" + thisrow;
	tdElement.className = "width200";

	// 5
	tdElement = document.createElement("td");
	if(isIE){
		selectElement = document.createElement("<select name='P_round_id[]'>");
	}else{
		selectElement = document.createElement("select");
		selectElement.name = "P_round_id[]";
	}
	selectElement.onkeydown=function(){return move_focus(event,this);}

	tdElement.appendChild(selectElement);

	selectElement.options[0] = new Option("",0);


	//選択されているラウンドを除外している？
	var option_cnt = 0;

	for(i=0;i<r_cnt;i++)
	{
		var check = false;

		for(j=0;j<sel_r_code.length;j++)
		{
			if(sel_r_code[j] == r_code[i])
			{
				check = true;
			}
		}
		if(check == false)
		{
			option_cnt = option_cnt + 1;
			selectElement.options[option_cnt] = new Option(r_name[i],r_code[i]);
		}
	}
	selectElement.value = "";
	selectElement.className = "selectNormal";


	//現在のラウンドと組数、進出条件を取得する
	var now_e_code = e_code[ins_index-1].value;
//	var now_round = round_id[ins_index-1].selectedIndex;
	var now_round = round_id[ins_index-1].value;
	var now_max = max_race[ins_index-1].value;
	var now_rank = advance_rank[ins_index-1].value;
	var now_num = advance_num[ins_index-1].value;
	var now_rec = advance_rec[ins_index-1].value;

	var next_max = 1;
	var next_rank = 0;
	var next_num = 0;

	//フィールド競技の場合、次のラウンドは必ず決勝
	if((now_e_code.substr(0,1)=="F")||(now_e_code.substr(0,1)=="R")||(now_e_code.substr(0,1)=="M")||(now_e_code.substr(0,1)=="T")||(now_e_code.substr(0,1)=="U")||(now_e_code.substr(0,1)=="Z"))
	{
		selectElement.value = 1;
	}
	else
	{
	//フィールド以外の場合

		if(now_rank=="")
			now_rank=0;

		if(now_num=="")
			now_num=0;

		//組数と進出条件で次の組の人数を計算する
		var adv_cnt = parseInt(now_max,10) * parseInt(now_rank,10) + parseInt(now_num,10);

		var max_adv = 10;
		var div_race = 4;
		var count_Q = 3;

		//種目に応じて進出人数を判断する
		if(((now_e_code.substr(0,3)>="A11")&&(now_e_code.substr(0,3)<"A12"))||(now_e_code.substr(0,2)=="AS")||((now_e_code.substr(0,1)=="D")&&(now_e_code.substr(0,3)>"D16")))
		{
//			max_adv = 15;
			max_adv = 12;
			div_race = 4;
			count_Q = 3;
		}
		else if((now_e_code.substr(0,3)>="A12")&&(now_e_code.substr(0,3)<"A20"))
		{
//			max_adv = 27;
			max_adv = 18;
			div_race = 4;
			count_Q = 3;
		}
		else if(((now_e_code.substr(0,3)>="A20")&&(now_e_code.substr(0,3)<="A99"))||(now_e_code.substr(0,2)=="AW")||(now_e_code.substr(0,2)=="AX"))
		{
//			max_adv = 30;
			max_adv = 18;
			div_race = 4;
			count_Q = 4;
		}
//		if((now_e_code.substr(0,2)=="A0")||(now_e_code.substr(0,2)=="AH")||(now_e_code.substr(0,2)=="AY")||((now_e_code.substr(0,1)=="D")&&(now_e_code.substr(0,3)<="D16")))
		else
		{
			max_adv = 8;
			div_race = 3;
			count_Q = 3;
		}


		var set_adv=1;
		var first_max = Math.ceil(parseInt(adv_cnt) / parseInt(max_adv,10));
		next_max =first_max;
//alert(next_max);

		//１組になったら決勝、ならなかったら更に割って１になるまで繰り返す
		while(first_max > 1)
		{
			//進出した人数と組内人数から組数を計算する
			first_max = Math.ceil(parseInt(first_max,10) / parseInt(div_race,10));
			set_adv++;
		}

		//決勝にならなかった場合、進出条件を抽出する
		if(set_adv>1)
		{
			//次の組数をdiv_raceで割った値がその次の組数となり、それにmax_advをかけた値が進出人数となる
			var next_next_max = Math.ceil(parseInt(next_max,10) / parseInt(div_race,10));
			var next_next_adv = next_next_max * max_adv;

			//次の組数から３着進出として余りの人数を求める
			next_rank=count_Q;
			next_num = next_next_adv - next_max * next_rank;

			//値がマイナスの場合、２着進出として計算しなおす
			if(next_num<0)
			{
				next_rank=2;
				next_num = next_next_adv - next_max * next_rank;
			}
			//それでもマイナスになる場合は（ないはずだが）ゼロにする
			if(next_num<0)
				next_num = 0;
		}

//alert(set_adv);
		//該当するものがあればセットする
		for(i=0;i<=option_cnt;i++)
		{
			if(set_adv == selectElement.options[i].value)
			{
				selectElement.value = set_adv;
				break;
			}
		}
	}



	trElement.appendChild(tdElement);
	tdElement.className = "width100";

	// 6
	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='text' name='P_max_race[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "text";
		inputElement.name = "P_max_race[]";
	}
	inputElement.value = next_max;

	inputElement.onkeydown=function(){return move_focus(event,this);}
	tdElement.appendChild(inputElement);
//	inputElement.value = 1;
	inputElement.className = "textBox imeOff width30";
	trElement.appendChild(tdElement);
	tdElement.className = "width60";

	// 7
	tdElement = document.createElement("td");
	if( document.form_main.P_search_event.value == 1 )
	{
		if(isIE){
			inputElement = document.createElement("<input type='hidden' name='P_advance_rank[]'>");
		}else{
			inputElement = document.createElement("input");
			inputElement.type = "hidden";
			inputElement.name = "P_advance_rank[]";
		}
		tdElement.appendChild(inputElement);
		inputElement.value = next_rank;

		if(isIE){
			inputElement = document.createElement("<input type='hidden' name='P_advance_num[]'>");
		}else{
			inputElement = document.createElement("input");
			inputElement.type = "hidden";
			inputElement.name = "P_advance_num[]";
		}
		tdElement.appendChild(inputElement);
		inputElement.value = next_num;
		if(isIE){
			inputElement = document.createElement("<input type='text' name='P_advance_rec[]'>");
		}else{
			inputElement = document.createElement("input");
			inputElement.type = "text";
			inputElement.name = "P_advance_rec[]";
		}
		inputElement.onkeydown=function(){return move_focus(event,this);}

		tdElement.appendChild(inputElement);
		inputElement.className = "textBox imeOff width50 ";
		textElement = document.createTextNode('m');
		tdElement.appendChild(textElement);
		trElement.appendChild(tdElement);

	}
	else
	{
		if(isIE){
			inputElement = document.createElement("<input type='text' name='P_advance_rank[]'>");
		}else{
			inputElement = document.createElement("input");
			inputElement.type = "text";
			inputElement.name = "P_advance_rank[]";
		}
		inputElement.onkeydown=function(){return move_focus(event,this);}

		tdElement.appendChild(inputElement);
		inputElement.value = next_rank;
		inputElement.className = "textBox imeOff width30";
		textElement = document.createTextNode('+');
		tdElement.appendChild(textElement);
		if(isIE){
			inputElement = document.createElement("<input type='text' name='P_advance_num[]'>");
		}else{
			inputElement = document.createElement("input");
			inputElement.type = "text";
			inputElement.name = "P_advance_num[]";
		}
		inputElement.onkeydown=function(){return move_focus(event,this);}

		tdElement.appendChild(inputElement);
		inputElement.value = next_num;
		inputElement.className = "textBox imeOff width30";
		if(isIE){
			inputElement = document.createElement("<input type='hidden' name='P_advance_rec[]'>");
		}else{
			inputElement = document.createElement("input");
			inputElement.type = "hidden";
			inputElement.name = "P_advance_rec[]";
		}
		tdElement.appendChild(inputElement);
	}
	trElement.appendChild(tdElement);
	tdElement.className = "width130";

	// 8
	tdElement = document.createElement("td");
	textElement = document.createTextNode('');
	tdElement.appendChild(textElement);
	trElement.appendChild(tdElement);
	tdElement.className = "width70";

	// 9
	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='button'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "button";
	}
	tdElement.appendChild(inputElement);
	tdElement.className = "";

	inputElement.name = "name_addb";
	inputElement.value = ' + ';
	inputElement.onclick = function(){add_rows(inputElement,thisrow);}
	inputElement.className = 'inputButton width50';
	trElement.appendChild(tdElement);
	trElement.id = 'id_row_' + thisrow;
	trElement.className = 'contentData';

	// 10
	tdElement = document.createElement("td");
	textElement = document.createTextNode('');
	tdElement.appendChild(textElement);
	trElement.appendChild(tdElement);
	tdElement.className = "";

	addrow = addrow + 1;
	document.form_main.P_game_race_rows.value = Number(document.form_main.P_game_race_rows.value) + 1;
}
function del_rows(obj,row)
{
	var delid = document.getElementById('id_pid_' + row).value;
	if( delid != "" )
	{
		if( confirm('削除しますか？（削除しても登録を実行するまで確定されません。）') == false )
			return false;

		document.getElementById('id_delchk_' + row).value = 'ON';
		document.getElementById('id_row_' + row).style.display = 'none';

		var target = document.getElementById('id_addb_' + row);
		target.parentNode.removeChild(target);
	}
	else
	{
		var target = document.getElementById('id_row_' + row);
		while(target.firstChild) target.removeChild(target.firstChild);
		target.parentNode.removeChild(target);

		document.form_main.P_game_race_rows.value = Number(document.form_main.P_game_race_rows.value) - 1;
	}
}

function onload_event()
{
	return;
}

function move_focus2(e,obj)
{
	if(e){
		code = e.keyCode;
		shif = e.shiftKey;
	}else{
		code = event.keyCode;
		shif = event.shiftKey;
	}
	if(code == 13){
		var target = document.form_main.elements[obj.name];
		var mov = 0;
		for(i=0;i<target.length;i++){
			if( obj == target[i] ){
				if(shif == false)
				{
					for(j=i+1;j<=target.length;j++)
					{
						mov = j;

						if( mov == target.length )
						{
							mov = 0; 
							break;

						}
						else if(target[mov].getAttribute("type") != 'hidden')
						{
						
							break;
						}
					 }
				}
				else
				{
					for(j=i;j>-1;j--)
					{
						mov = j - 1;
						if( mov == -1 )
						{
							for(k=target.length-1;k>-1;k--)
							{
								if(target[k].getAttribute("type") != 'hidden')
								{
									mov = k; 
									break;
								}
							}
							break;
						}
						else 
						{
							break;
						}
					}
				}
				break;
			}
		}
            
		target[mov].focus();
		if( target[mov].type == "text" || target[mov].type == "textarea" || target[mov].type == "password" )
			target[mov].select();

		return false;
	}else{
		return true;
	}
	return false;
}

var print_post_data = '';
function print_button_click(print_style)
{

	var key_event = window.event;

	if(key_event.ctrlKey)
	{
		print_button_excel(2);
		return false;
	}

	document.getElementById('id_print_button').style.display = '';

	var sex = 0;
	var e_class = 0;
	var e_comb = 0;
	var e_code = "";
	var round_id = 0;
	var race_start = 0;
	var race_end = 0;
	var job_parameters = "";

	print_post_data = encodeURIComponent("userid") + "=" + encodeURIComponent('ay02');
	print_post_data += "&" + encodeURIComponent("game_num") + "=" + encodeURIComponent('ay022022015');
	print_post_data += "&" + encodeURIComponent("stadium_id") + "=" + encodeURIComponent('17');
	print_post_data += "&" + encodeURIComponent("print_terminal_id") + "=" + encodeURIComponent(document.getElementById('id_select_print_terminal').value);
	print_post_data += "&" + encodeURIComponent("print_style") + "=" + encodeURIComponent(print_style);
	print_post_data += "&" + encodeURIComponent("sex") + "=" + encodeURIComponent(sex);
	print_post_data += "&" + encodeURIComponent("e_class") + "=" + encodeURIComponent(e_class);
	print_post_data += "&" + encodeURIComponent("e_comb") + "=" + encodeURIComponent(e_comb);
	print_post_data += "&" + encodeURIComponent("e_code") + "=" + encodeURIComponent(e_code);
	print_post_data += "&" + encodeURIComponent("round_id") + "=" + encodeURIComponent(round_id);
	print_post_data += "&" + encodeURIComponent("race_start") + "=" + encodeURIComponent(race_start);
	print_post_data += "&" + encodeURIComponent("race_end") + "=" + encodeURIComponent(race_end);
	print_post_data += "&" + encodeURIComponent("job_parameters") + "=" + encodeURIComponent(job_parameters);


	print_job_click();
}
function print_job_post()
{
	return print_post_data;
}

var form_element = new Array('P_round_id[]','P_max_race[]','P_advance_rank[]','P_advance_num[]','P_advance_rec[]');
function is_change()
{

	// 指定フォーム要素対象版
	var cnt = form_element.length;
	for(var i=0;i<cnt;i++)
	{
		if( document.form_main.elements[form_element[i]] )
		{
			var frm = document.form_main.elements[form_element[i]];

			var tmptag = document.getElementsByName( form_element[i] ) ;


			if( frm.length > 1 )
			{
				for(var j=0;j<frm.length;j++)
				{
					if( frm[j].tagName == "INPUT" )
					{
						if( frm[j].getAttribute("type") == "text" && frm[j].value != frm[j].defaultValue )
						{
							return true; // 変更あり
						}
						else if( frm[j].getAttribute("type") == "checkbox" && frm[j].checked != frm[j].defaultChecked )
							return true; // 変更あり
						else if( frm[j].getAttribute("type") == "radio" && frm[j].checked != frm[j].defaultChecked )
							return true; // 変更あり
					}
					else if( frm[j].tagName == "SELECT" && frm[j].options[frm[j].selectedIndex].defaultSelected == false )
					{
						return true; // 変更あり
					}
					else if( frm[j].tagName == "TEXTAREA" && frm[j].value != frm[j].defaultValue )
						return true; // 変更あり
				}
			}
			else
			{

				if( frm.tagName == "INPUT" )
				{
					if( frm.getAttribute("type") == "text" && frm.value != frm.defaultValue )
					{
						return true; // 変更あり
					}
					else if( frm.getAttribute("type") == "checkbox" && frm.checked != frm.defaultChecked )
						return true; // 変更あり
					else if( frm.getAttribute("type") == "radio" && frm.checked != frm.defaultChecked )
						return true; // 変更あり
				}
				else if( frm.tagName == "SELECT" && frm.options[frm.selectedIndex].defaultSelected == false )
				{
					return true; // 変更あり
				}
				else if( frm.tagName == "TEXTAREA" && frm.value != frm.defaultValue )
					return true; // 変更あり
			}
		}
	}


	return false;
}


