function tab_click(var_x)
{
	document.getElementById("id_base_tab").style.display = "none";
	document.getElementById("id_release_tab").style.display = "none";
	document.getElementById("id_manage_tab").style.display = "none";
	document.getElementById("id_class_tab").style.display = "none";
	document.getElementById("id_entry_tab").style.display = "none";
	document.getElementById("id_system_tab").style.display = "none";
	document.getElementById("id_outsource_tab").style.display = "none";

	document.getElementById("id_tab_1").className = "width13per tabbtn";
	document.getElementById("id_tab_2").className = "width13per tabbtn";
	document.getElementById("id_tab_3").className = "width13per tabbtn";
	document.getElementById("id_tab_4").className = "width13per tabbtn";
	document.getElementById("id_tab_5").className = "width13per tabbtn";
	document.getElementById("id_tab_6").className = "width13per tabbtn";
	document.getElementById("id_tab_7").className = "width13per tabbtn";

	document.form_main.P_tab_select_id.value = var_x;

	switch(var_x){
		case 1: //競技会登録＿基本情報タブを表示
			document.getElementById("id_tab_1").className = "width13per backDBlue";
			document.getElementById("id_base_tab").style.display = "";
			break;
		case 2: //競技会登録＿公開設定タブを表示
			document.getElementById("id_tab_2").className = "width13per backDBlue";
			document.getElementById("id_release_tab").style.display = "";
			break;
		case 3: //競技会登録＿競技会運営タブを表示
			document.getElementById("id_tab_3").className = "width13per backDBlue";
			document.getElementById("id_manage_tab").style.display = "";
			break;
		case 4: //競技会登録＿クラスタブを表示
			document.getElementById("id_tab_4").className = "width13per backDBlue";
			document.getElementById("id_class_tab").style.display = "";
			break;
		case 5: //競技会登録＿エントリータブを表示
			document.getElementById("id_tab_5").className = "width13per backDBlue";
			document.getElementById("id_entry_tab").style.display = "";
			break;
		case 6: //競技会登録＿システムタブを表示
			document.getElementById("id_tab_6").className = "width13per backDBlue";
			document.getElementById("id_system_tab").style.display = "";
			break;
		case 7: //競技会登録＿外部サービスタブを表示
			document.getElementById("id_tab_7").className = "width13per backDBlue";
			document.getElementById("id_outsource_tab").style.display = "";
			break;
		default:
			break;
	}
	return false;
}
function button_click(var_x,var_y)
{
	if( var_x != 1 )
		var change_flag = is_change();

	document.form_main.target = "";

	document.form_main.P_submit_value.value = "";
	switch(var_x){
		case 9999:
			document.form_main.action = "../error.php";
			break;
		case 'back': //戻る

			if( change_flag == true )
			{
				if( confirm('更新して移動しますか？') == true )
				{
					document.form_main.P_back_flag.value = 1;
					button_click(1,0);
					return false;
				}
				else
				{
					if( confirm('更新せずに移動しますか？') == false )
						return false;
				}
			}
			document.form_main.action = "./0011_gamemenu.php";

			break;
		case 1: //競技会登録：登録処理
			if( postdata_check() == false )
				return false;
			document.form_main.P_submit_value.value = "REGIST GAME_TABLE ALL";
			document.form_main.action = "./0012_gameinput.php";
			break;
		case 2: //競技会登録：登録処理
			if( confirm('この大会を削除してよろしいですか？\nこの大会に登録されている情報も合わせて削除します。') == true )
			{
				if( confirm('復活はできません。\n本当に削除してよろしいですか？') == true )
				{
					if( confirm('最終確認です。\n本当に削除しますか？') == true )
					{
						document.form_main.P_submit_value.value = "DELETE GAME_TABLE ALL";
						document.form_main.action = "./0012_gameinput.php";
					}
					else
						return false;
				}
				else
					return false;
			}
			else
				return false;
			break;
		case 3: //競技会登録：他大会取込
			document.form_main.target = "_blank";
			document.form_main.P_game_copy.value ="COPY";
			document.form_main.action = "./0003_gamelist.php";
			break;

		case 4: //競技会登録：略号デフォルト登録
			if( confirm('略語を削除して、初期値の略語を登録します。よろしいですか？') == false )
				return false;

			document.form_main.P_submit_value.value = "REGIST LEGEND DEFAULT";
			document.form_main.action = "./0012_gameinput.php";
			break;

		case 5: //競技会登録：直近略号登録
			if( confirm('略語を削除して、直近の競技会の略語を登録します。よろしいですか？') == false )
				return false;

			document.form_main.P_submit_value.value = "REGIST LEGEND GAME";
			document.form_main.action = "./0012_gameinput.php";
			break;

		default:
			document.form_main.action = "../index.php";
			break;
	}
	document.form_main.submit();
	return false;
}

function button_link(var_x)
{
	document.form_main.P_submit_value.value = "";
	switch(var_x){

		case 1:
			document.form_main.action = "../../ii/imb_ver3.php?spp=&stp=1";
			document.form_main.shortcut_flag.value = 1;
			document.form_main.target = "_blank";
			break;
		case 2:
			document.form_main.action = "../../ii/imb_ver3.php?spp=&stp=2";
			document.form_main.shortcut_flag.value = 1;
			document.form_main.target = "_blank";
			break;
		case 3:
			document.form_main.action = "../../ii/imb_ver3.php?spp=&stp=3";
			document.form_main.shortcut_flag.value = 1;
			document.form_main.target = "_blank";
			break;

		default:
			document.form_main.action = "../index.php";
			break;
	}
	document.form_main.submit();
	return false;
}

function button_click_popup(var_x,var_y)
{
	switch(var_x){
		case 1: //基本競技会選択画面を別窓で表示
			document.form_main.target = 'ARV3_WINDOW';
			document.form_main.P_submit_sub.value = "TAB SELECT MODE";
			document.form_main.action = "./0004_gamebaselist.php";
			break;
		case 2: //競技場選択画面を別窓で表示
			document.form_main.target = 'ARV3_WINDOW';
			document.form_main.P_submit_sub.value = "TAB SELECT MODE";
			document.form_main.action = "./0006_gameplacelist.php";
			break;
		default:
			return false;
			break;
	}
	document.form_main.submit();
	document.form_main.target = '';
	document.form_main.P_submit_sub.value = '';
	return false;
}

function postdata_check()
{
	var data_req = document.form_main.P_start_date.value;
	if( data_req == '' )
	{
		alert('競技会開催期間：必須項目です。入力してください。');
		return false;
	}
	data_req = document.form_main.P_end_date.value;
	if( data_req == '' )
	{
		alert('競技会開催期間：必須項目です。入力してください。');
		return false;
	}
	data_req = document.form_main.P_short_name.value;
	if( data_req == '' )
	{
		alert('競技会略称（16文字以内）：必須項目です。入力してください。');
		return false;
	}
	else if( data_req.length > 16 )
	{
		alert('競技会略称（16文字以内）：指定桁数・文字数以内で入力してください。');
		return false;
	}
	return true;
}
var addrow_class = 1;
function add_class_rows(obj,row)
{
	var isIE =/*@cc_on!@*/false;
	var tbodyElement,trElement,tdElement,inputElement;
	var thisrow = addrow_class;


	var totalrow = document.form_main.P_class_rows.value;
	var max_num = 0;
	var elements = document.getElementsByName( "P_class_table_sort_num[]" ) ;
	for(var i=0;i<totalrow;i++)
	{
		if(max_num < parseInt(elements[i].value, 10))
		{
			max_num = parseInt(elements[i].value, 10);
		}

	}




	tbodyElement = document.getElementById("id_class_tbody");
	trElement = document.createElement("tr");
	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='hidden' name='P_class_table_id[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "hidden";
		inputElement.name = "P_class_table_id[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.id = "id_class_pid_" + thisrow;
	inputElement.value = "";

	if(isIE){
		inputElement = document.createElement("<input type='hidden' name='P_class_table_delete_check[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "hidden";
		inputElement.name = "P_class_table_delete_check[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.value = "";

	if(isIE){
		inputElement = document.createElement("<input type='button'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "button";
	}
	tdElement.appendChild(inputElement);
	inputElement.id = "id_class_delb_" + thisrow;
	inputElement.value = "削除";
	inputElement.onclick = function(){del_class_rows(inputElement,thisrow);}
	inputElement.className = "inputButton width50";
	trElement.appendChild(tdElement);
	tdElement.className = "width75 paddingTB3 paddingL15 ";

	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='text' name='P_class_table_sort_num[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "text";
		inputElement.name = "P_class_table_sort_num[]";
	}

	max_num++;

	tdElement.appendChild(inputElement);
	inputElement.value = max_num;
	inputElement.onkeydown = function(){return move_focus(event,this);}
	inputElement.className = "textBox imeOff width50";
	trElement.appendChild(tdElement);
	tdElement.className = "width75 paddingTB3";

	tdElement = document.createElement("td");
	trElement.appendChild(tdElement);
	tdElement.className = "width75 paddingTB3";

	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='text' name='P_class_table_name_class[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "text";
		inputElement.name = "P_class_table_name_class[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.value = "";
	inputElement.onkeydown = function(){return move_focus(event,this);}
	inputElement.className = "textBox width250";
	trElement.appendChild(tdElement);
	tdElement.className = "width300 paddingTB3";

	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='text' name='P_class_table_name_english[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "text";
		inputElement.name = "P_class_table_name_english[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.value = "";
	inputElement.onkeydown = function(){return move_focus(event,this);}
	inputElement.className = "textBox width250";
	trElement.appendChild(tdElement);
	tdElement.className = "width300 paddingTB3";

	tdElement = document.createElement("td");
	if(isIE){
		inputElement = document.createElement("<input type='button'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "button";
	}
	tdElement.appendChild(inputElement);
	inputElement.name = "name_class_addb";
	inputElement.value = ' + ';
	inputElement.onclick = function(){add_class_rows(inputElement,thisrow);}
	inputElement.className = 'inputButton width50';
	trElement.appendChild(tdElement);
	tbodyElement.appendChild(trElement);
	trElement.id = 'id_class_row_' + thisrow;
	trElement.className = 'contentData';

	addrow_class = addrow_class + 1;
	document.form_main.P_class_rows.value = Number(document.form_main.P_class_rows.value) + 1;
	obj.style.display = 'none';
}
function del_class_rows(obj,row)
{
	var delid = document.getElementById('id_class_pid_' + row).value;
	if( delid != "" )
	{
		if( confirm('削除しますか？（削除しても登録を実行するまで確定されません。）') == false )
			return false;

		document.getElementById('id_class_delchk_' + row).value = 'ON';
		document.getElementById('id_class_row_' + row).style.display = 'none';

		var target = document.getElementById('id_class_addb_' + row);
		target.parentNode.removeChild(target);
	}
	else
	{
		var target = document.getElementById('id_class_row_' + row);
		while(target.firstChild) target.removeChild(target.firstChild);
		target.parentNode.removeChild(target);

		document.form_main.P_class_rows.value = Number(document.form_main.P_class_rows.value) - 1;
	}
	var btn = document.getElementsByName('name_class_addb');
	btn[btn.length-1].style.display = '';

}
function userbutton_click(var_x)
{
	switch(var_x){
		case 1:
			if( confirm('入力した競技会IDに一致する、ID、パスワードを取得しますか？\n（取得しても登録を実行するまで確定されません。）') == false )
				return false;

			ajax_mod(1);
			break;
		case 2:
			if( confirm('ID、パスワードを自動で生成しますか？\n（生成しても登録を実行するまで確定されません。）') == false )
				return false;

			ajax_mod(2);
			break;
		case 3:
			if( confirm('ID、パスワードを削除しますか？\n（削除しても登録を実行するまで確定されません。）') == false )
				return false;

			clear_game_id();
			break;
	}
	return false;
}
function ajax_mod(var_x)
{
	var poststr = '';
	var val1 = document.form_main.P_SESSION_KEY.value;
	var val2 = document.form_main.P_SESSION_VALUE.value;
	var val3 = document.form_main.P_prev_page.value;
	var val4 = document.form_main.P_select_game_table_id.value;

	poststr = encodeURIComponent("P_SESSION_KEY") + "=" + encodeURIComponent(val1);
	poststr += "&" + encodeURIComponent("P_SESSION_VALUE") + "=" + encodeURIComponent(val2);
	poststr += "&" + encodeURIComponent("P_prev_page") + "=" + encodeURIComponent(val3);
	poststr += "&" + encodeURIComponent("P_select_game_table_id") + "=" + encodeURIComponent(val4);
	if( var_x == 1 )
	{
		poststr += "&" + encodeURIComponent("P_submit_value") + "=" + encodeURIComponent("GETGUID GAME_TABLE SYSTEM");
		poststr += "&" + encodeURIComponent("P_search_game_num") + "=" + encodeURIComponent(document.form_main.P_search_game_num.value);
	}
	else if( var_x == 2 )
		poststr += "&" + encodeURIComponent("P_submit_value") + "=" + encodeURIComponent("CREATEGUID GAME_TABLE SYSTEM");
	else if( var_x == 3 )
		poststr += "&" + encodeURIComponent("P_submit_value") + "=" + encodeURIComponent("COPYTERMINAL GAME_TABLE SYSTEM");

	var xmlhttp1 = this.XDomainRequest ? new XDomainRequest() : new XMLHttpRequest();
	xmlhttp1.onreadystatechange = function()
	{
		if( xmlhttp1.readyState == 4 )
		{
			if( xmlhttp1.status == 200 )
			{
				var result = xmlhttp1.responseText;
				try {
					var json_obj = JSON.parse(result);
				} catch(e) {
					var json_obj = false;
				}

				if( json_obj )
				{
					//if(console && console.log) { console.log(json_obj); };
					switch(json_obj["status"])
					{
						case "OK":
							try {
								if( json_obj["result"] == 'Complete' )
								{
									if( var_x == 3 )
										set_terminal_id(json_obj);
									else
										set_game_id(json_obj);
								}
								else
									alert('該当データがありませんでした。');
							} catch(e) {
								alert('データ通信に失敗しました。');
							}
							break;
						default:
							alert('データ通信に失敗しました。');
							break;
					}
				}
				else
				{
					alert('データ通信に失敗しました。');
				}
			}
		}
	}
	xmlhttp1.open("POST", "./0012_gameinput.php", true);
	xmlhttp1.setRequestHeader('Content-Type','application/x-www-form-urlencoded');
	xmlhttp1.send(poststr.replace(/%20/g,'+'));
}
function set_game_id(json_obj)
{
	for(var i=0;i<5;i++)
	{
		document.form_main.elements['P_user_table_id[]'][i].value = '';
		document.form_main.elements['P_user_table_loginid[]'][i].value = json_obj['user_table']['loginid'][i];
		document.form_main.elements['P_user_table_loginpw[]'][i].value = json_obj['user_table']['loginpw'][i];
	}
}
function clear_game_id()
{
	for(var i=0;i<5;i++)
	{
		document.form_main.elements['P_user_table_id[]'][i].value = '';
		document.form_main.elements['P_user_table_loginid[]'][i].value = '';
		document.form_main.elements['P_user_table_loginpw[]'][i].value = '';
	}
}
function copyterminalbutton_click()
{
	if( confirm('直近の同競技場の競技会から端末情報を取得しますか？\n（取得しても登録を実行するまで確定されません。）') == false )
		return false;

	ajax_mod(3);
}
var nowrow_terminal = 1;
var addrow_terminal = 1;

var nowrow_legend = 1;
var addrow_legend = 1;


function set_terminal_id(json_obj)
{
	if( json_obj['print_terminal'] )
	{
		var cnt = json_obj['print_terminal']['id'].length;
		for(var i=0;i<cnt;i++)
		{
			if( i >= nowrow_terminal )
			{
				var btn = document.getElementsByName('name_terminal_addb');
				btn[i-1].onclick();
			}
			if( nowrow_terminal == 1 )
				document.form_main.elements['P_terminal_name[]'].value = json_obj['print_terminal']['terminal_name'][i];
			else
				document.form_main.elements['P_terminal_name[]'][i].value = json_obj['print_terminal']['terminal_name'][i];
		}
		for(i=nowrow_terminal;i>cnt;i--)
		{
			var btn = document.getElementsByName('name_terminal_delb');
			btn[i-2].onclick();
		}
	}
}
function add_terminal_rows(obj,row)
{
	var isIE =/*@cc_on!@*/false;
	var tbodyElement,trElement,tdElement,inputElement,spanElement,textElement;
	var thisrow = addrow_terminal;

	tbodyElement = document.getElementById("id_system_tbody");

	if(row > 0)
	{
		for(i=0;i<tbodyElement.rows.length;i++)
		{
			if( tbodyElement.rows[i].id == "id_terminal_new_row_" + row )
			{
				ins_index = i;
				break;
			}
		}
	}
	else
	{
		for(i=0;i<tbodyElement.rows.length;i++)
		{
			if( tbodyElement.rows[i].id == "id_terminal_row" )
			{
				ins_index = i;
				break;
			}
		}

	}

	trElement = tbodyElement.insertRow(ins_index + 1);

//	trElement = document.createElement("tr");
//	tdElement = document.createElement("td");

//	trElement = document.createElement("tr");
	tdElement = document.createElement("td");

	if(isIE){
		spanElement = document.createElement("<span>");
	}else{
		spanElement = document.createElement("span");
	}
	if(isIE){
		inputElement = document.createElement("<input type='button'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "button";
	}
	spanElement.appendChild(inputElement);
	inputElement.id = "id_terminal_delb_" + thisrow;
	inputElement.name = "name_terminal_delb";
	inputElement.value = "削除";
	inputElement.onclick = function(){del_terminal_rows(inputElement,thisrow);}
	inputElement.className = "inputButton width50";

	tdElement.appendChild(spanElement);
	spanElement.className = "inlineBox width75";
	textElement = document.createTextNode(' ');
	tdElement.appendChild(textElement);

	if(isIE){
		inputElement = document.createElement("<input type='text' name='P_terminal_name[]'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "text";
		inputElement.name = "P_terminal_name[]";
	}
	tdElement.appendChild(inputElement);
	inputElement.value = "";
	inputElement.onkeydown = function(){move_focus_name_array(event,"P_auth_key_walk","P_game_status",this);}
	inputElement.className = "textBox width300";
	textElement = document.createTextNode(' ');
	tdElement.appendChild(textElement);

	if(isIE){
		inputElement = document.createElement("<input type='button'>");
	}else{
		inputElement = document.createElement("input");
		inputElement.type = "button";
	}
	tdElement.appendChild(inputElement);
	inputElement.name = "name_terminal_addb";
	inputElement.value = ' + ';
	inputElement.onclick = function(){add_terminal_rows(inputElement,thisrow);}
	inputElement.className = 'inputButton width50';

	trElement.appendChild(tdElement);
//	tbodyElement.appendChild(trElement);
	trElement.id = 'id_terminal_new_row_' + thisrow;

	var trobj = document.getElementById("id_terminal_th");
	trobj.rowSpan = Number(trobj.rowSpan) + 1;


	addrow_terminal++;
	nowrow_terminal++;
	document.form_main.P_new_terminal_rows.value = Number(document.form_main.P_new_terminal_rows.value) + 1;
	obj.style.display = 'none';
}
function del_terminal_rows(obj,row)
{
	var target = document.getElementById('id_terminal_new_row_' + row);
	while(target.firstChild) target.removeChild(target.firstChild);
//	target.parentNode.removeChild(target);

	document.form_main.P_new_terminal_rows.value = Number(document.form_main.P_new_terminal_rows.value) - 1;
	var btn = document.getElementsByName('name_terminal_addb');
	btn[btn.length-1].style.display = '';
	nowrow--;
}
function hidden_terminal_rows(obj,row)
{
	if( confirm('端末情報を削除しますか？\n（削除しても登録を実行するまで確定されません。）') == false )
		return false;

	document.getElementById('id_terminal_delchk_' + row).value = 'ON';
	document.getElementById('id_terminal_row_' + row).style.display = 'none';

	var trobj = document.getElementById("id_terminal_th");
	trobj.rowSpan = trobj.rowSpan - 1;
}


