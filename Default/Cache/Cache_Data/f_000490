function is_change()
{
	// 指定フォーム要素対象版
	var cnt = document.form_main.length;

	for(var i=0;i<cnt;i++)
	{
		var frm = document.form_main.elements[i];
		if( frm.tagName == "INPUT" )
		{
			if( frm.getAttribute("type") == "text" && frm.value != frm.defaultValue )
			{

				return true; // 変更あり
			}
			else if( frm.getAttribute("type") == "checkbox" && frm.checked != frm.defaultChecked )
			{
				return true; // 変更あり
			}
			else if( frm.getAttribute("type") == "radio" && frm.checked != frm.defaultChecked )
			{
				return true; // 変更あり
			}
		}
		else if( frm.tagName == "SELECT" )
		{
			if(frm.options[frm.selectedIndex].defaultSelected == true)
				continue; // 変更なし
			else if(frm.selectedIndex == 0)
			{
				var option_cnt = frm.options.length;

				for(var j=0;j<option_cnt;j++)
				{

					if(frm.options[j].defaultSelected == true)
						return true; // 変更あり
				}
				continue; // 変更なし
			}
			else
				return true; // 変更あり

		}
		else if( frm.tagName == "TEXTAREA" && frm.value != frm.defaultValue )
		{
			return true; // 変更あり
		}
	}
	return false;
}


function button_copy()
{


	//着順、レーン、記録をコピーします
	var cnt = 68;

	buf = "";

	var delete_check = document.getElementsByName( "P_print_legend_delete_check[]" ) ;
	var irm_code = document.getElementsByName( "P_print_legend_irm_code[]" ) ;
	var jpn_text = document.getElementsByName( "P_print_legend_irm_jpn_text[]" ) ;
	var eng_text = document.getElementsByName( "P_print_legend_irm_eng_text[]" ) ;
	var new_irm_code = document.getElementsByName( "P_new_irm_code[]" ) ;
	var new_jpn_text = document.getElementsByName( "P_new_irm_jpn_text[]" ) ;
	var new_eng_text = document.getElementsByName( "P_new_irm_eng_text[]" ) ;

	var cnt2 = document.form_main.P_new_legend_rows.value;

	//タイトルをつけます


	//レーンとナンバーと記録を取得する
	for(i=0;i<cnt;i++)
	{
		if(delete_check[i].value == "" )
		{
			buf = buf + irm_code[i].value + "\t";
			buf = buf + jpn_text[i].value + "\t";
			buf = buf + eng_text[i].value + "\r\n";
		}
	}

	for(i=0;i<cnt2;i++)
	{
		buf = buf + new_irm_code[i].value + "\t";
		buf = buf + new_jpn_text[i].value + "\t";
		buf = buf + new_eng_text[i].value + "\r\n";
	}

	var copyarea = document.getElementById("copyarea");

    copyarea.value = buf;
    copyarea.select();
    document.execCommand("copy");

	alert("コピーしました。");

	return;
}


