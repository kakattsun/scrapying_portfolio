var default_legend_code = new Array('FS','T1','T2','T3','T4','T5','T6','T7','T8','R1','K1','K2','K3','K4','K5','YC','YRC','RC','SB','PB','R1.1','R1.2','R1.3','R2','R3','T9','T10','T11','L','L2');
